@{
    ViewBag.Title = "Index";
}


        <!--Список-->
        <h1>@ViewData["Greeting"]</h1>
        <div class="list" id="listId">
            <h2>Список</h2>

            <ol type="A">
                <li>Элемент 0 списка</li>
                <li>Элемент 1 списка</li>
                <li>Элемент 2 списка</li>
                <li>Элемент 3 списка</li>
                <li>Элемент 4 списка</li>
                <li>Элемент 5 списка</li>
            </ol>
        </div>

        <hr />

        <form method="post">
            <h3>Форма</h3>
            <div class="col-md-4">
                <label class="visually-hidden" for="autoSizingSelect">Preference</label>
                <select class="form-select" id="autoSizingSelect">
                    <option selected>Choose...</option>
                    <option value="1">Item_1</option>
                    <option value="2">Item_2</option>
                    <option value="3">Item_3</option>
                </select>
            </div>
            <div class="col-12">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" value="" id="flexCheck1">
                    <label class="form-check-label" for="flexCheckDefault1">
                        Чек-бокс 1
                    </label>
                </div>
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" value="" id="flexCheck2">
                    <label class="form-check-label" for="flexCheckDefault2">
                        Чек-бокс 2
                    </label>
                </div>
            </div>
            <div class="col-12">
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
                    <label class="form-check-label" for="inlineRadio1">Радио-1</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
                    <label class="form-check-label" for="inlineRadio2">Радио-2</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3">
                    <label class="form-check-label" for="inlineRadio3">Радио-3</label>
                </div>
                <div class="col-md-4">
                    <label for="exampleInputEmail1" class="form-label">Логин</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                    <div id="emailHelp" class="form-text">Мы никогда не передадим ваш адрес электронной почты кому-либо еще.</div>
                </div>
                <div class="col-md-4">
                    <label for="exampleInputPassword1" class="form-label">Пароль</label>
                    <input type="password" class="form-control" id="exampleInputPassword1">
                </div>
                <div id="passwordHelpBlock" class="form-text">
                    Ваш пароль должен быть длиной от 8 до 20 символов, содержать <br> буквы и цифры и не должен содержать пробелов, специальных символов или эмодзи.
                </div>
                <button type="submit" class="btn btn-primary mb-3">Отправить</button>
            </div>
           
        </form>

    